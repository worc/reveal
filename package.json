{
  "name": "reveal",
  "version": "0.0.0",
  "description": "Chrome and Firefox extensions to reveal password inputs with a hotkey",
  "main": "index.js",
  "scripts": {
    "build:chrome": "tsc --build chrome.tsconfig.json",
    "build:firefox": "tsc --build firefox.tsconfig.json",
